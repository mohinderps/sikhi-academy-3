{
  "requiresCompatibilities": ["FARGATE"],
  "family": "sikhi-academy-backend",
  "containerDefinitions": [
    {
      "name": "sikhi-academy-backend",
      "image": "",
      "essential": true,
      "portMappings": [
        {
          "name": "3000",
          "containerPort": 3000,
          "hostPort": 3000,
          "protocol": "tcp",
          "appProtocol": "http"
        }
      ]
    }
  ],
  "volumes": [],
  "networkMode": "awsvpc",
  "memory": "3 GB",
  "cpu": "1 vCPU",
  "executionRoleArn": "arn:aws:iam::702950744098:role/ecsTaskExecutionRole"
}
